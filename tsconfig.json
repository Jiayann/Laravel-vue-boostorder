{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "baseUrl": "./",
        "moduleResolution": "node",
        "paths": {
            "@/*":[
                "src/*"
            ]
        },
        
        "lib": [
            "esnext",
            "dom",
            "dom.iterable",
            "scripthost"
        ]
    },

    "vueCompilerOptions": {
        "target": 2.7
    },

    "include": ["src/**/*"],
    "exclude": ["file.ts"]

}
